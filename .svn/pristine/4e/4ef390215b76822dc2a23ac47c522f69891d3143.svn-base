.city-list>ul>li:active>a{color: #fff;}

/*搜索框样式*/
.header{height: 40px;line-height: 40x;background: #fff;position: fixed;top: 0;left: 0;right: 0;width: 100%;z-index: 12;padding: 0 10px;}
.header .header-left{position: absolute;top: 0;left: 10px;line-height: 40px;}
.header .search{position: absolute;left: 35px;right: 40px;top: 5px;line-height: 30px;font-size: 12px;border: 1px solid rgba(0, 0, 0, 0.1);height: 30px;border-radius: 5px;}
