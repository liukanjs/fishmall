/*微商城*/
/*html.wx{background: #fff;}*/
html.wx body,html.m body{background: #f2f2f2;min-width: 300px;max-width: 720px; margin: 0 auto;;position: relative;}
.wx-header{display: table;width: 100%;text-align: center;}
.wx-header div{position: relative;}
.wx-header div.active{color: #509ee0;}
.wx-header div.active:before{content: "";height: 3px;bottom: 0;left: 0;right: 0;background: #509ee0;position: absolute;}
.wx .banner{min-width: 300px;}
.wx-footer{position: fixed;bottom: 0;right: 0;left: 0;background: #509ee0;z-index: 9;}
.app-footer{position: fixed;bottom: 0;right: 0;left: 0;;z-index: 9;}
.bug-footer{height: 150px;}
ul.wx-home-list{position: relative;list-style: none;padding-left: 0;background: #fff;}
.wx-sub-tit{position: absolute; top: -10px;margin-left: 12px;background: #fff;  z-index: 2;}
ul.wx-home-list li{margin:15px 20px 0 20px;position: relative;}
ul.wx-home-list li:first-child{margin-top: 20px;}
ul.wx-home-list li.boxshow:first-child{margin-top: 0;}
ul.wx-home-list li.w50{width: 50%;float: left;margin: 0;}
.countdown{position: absolute;right: 0;top: 0;background: rgba(0,0,0,0.5);color: #fff;padding: 1px 5px; border-radius: 10px 0 0 10px;}
.wx-cart-icon{position: absolute;right: 20px;bottom: 20px;background: #509ee0;color: #fff;border-radius: 50%;width: 30px;height: 30px;line-height: 30px;text-align: center;}
.wx-menu a{color:#ddd;}
.wx-menu a.active{color: #fff;font-weight: bold;}
.text-em{text-decoration: line-through;color: #999;}
ul.wx-list-top{display: table;width: 100%;padding-left: 0;list-style: none;}
ul.wx-list-top>li{display: table-cell;width:25%;color:#999;text-align: center;line-height: 30px;border-right: 1px #ddd solid;border-bottom: 1px #ddd solid;}
ul.wx-list-top>li.active,ul.wx-list-top>li.current{color: #509ee0;font-weight: bold;}
.blank{height: 40px;}
.wx-cart-num{width: 40%;display: table;border: 1px solid #ddd;/*position: absolute;left: 0;bottom: 0;*/}
.wx-cart-num>span{display: table-cell;text-align: center;padding: 2px 0;}
.wx-cart-num>span.wx-cart-num-remove{border-right: 1px solid #ddd;background: #f2f2f2;}
.wx-cart-num>span.wx-cart-num-add{border-left: 1px solid #ddd;background: #f2f2f2;}
.cart-height{min-height: 120px;position: relative;}
.cart-list-main{margin:0 20px 0 90px;line-height: 16px;}
ul.wx-home-list>li.item{margin:10px 0 0 0;  position: relative;border-bottom: 1px solid #ddd;}
/*ul.wx-home-list>li.item:last-child{border-bottom: none;}*/
.h80{height: 80px;position: relative;overflow: hidden;}
.wx-money{position:absolute;right: 10px;bottom: 10px;color: #509ee0;}
.wx-remove{position:absolute;right: 10px;top: 5px;color: #fff;background: #ddd;padding: 5px;  border-radius: 0 0 0 50%;}
.jf-exchange{position: absolute;bottom: 0;right: 0;}
.wx-userindex{position: relative; background: url(../images/wx-userindex.jpg) center center no-repeat;background-size: cover;}
