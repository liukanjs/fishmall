.header .header-right{position: absolute;top: 5px;right: 10px;}
.header .header-right .search_buttom{font-size: 1rem;height: 30px;width: 45px;background: #509ee0;color: #fff;border: none;border-radius: 0 5px 5px 0;vertical-align: middle;}
.keyword-list{padding-left: 0;}
.keyword-list li{position:relative;display:inline-block;width:auto;border:solid 1px #ddd;color:#666;border-radius:4px;margin:0 5px 8px 0;padding:3px 9px;cursor:pointer;}
