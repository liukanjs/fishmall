/*钱包*/
.card-header{width: 100%;background: -moz-linear-gradient(top, #4d4e4d, #363836);background: -webkit-gradient(linear, left top, left bottom, from(#4d4e4d), to(#363836));-webkit-box-shadow: inset 0 1px rgba(93, 94, 93, 1), inset 0 -1px rgba(72, 74, 72, 1), inset 1px 0 rgba(0, 0, 0, 0), inset -1px 0 rgba(0, 0, 0, 0);border-bottom: 1px solid #000000;border-top: 1px solid #000000;}
