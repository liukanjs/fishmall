.card-bg-main{width: 100%;background: -moz-linear-gradient(top, #ff5496, #fe1773);background: -webkit-gradient(linear, left top, left bottom, from(#ff5496), to(#fe1773));-webkit-box-shadow: inset 0 1px rgba(93, 94, 93, 1), inset 0 -1px rgba(72, 74, 72, 1), inset 1px 0 rgba(0, 0, 0, 0), inset -1px 0 rgba(0, 0, 0, 0);}
.qmx-bg-main{width: 100%;background: -moz-linear-gradient(top, #ffb84f, #ff8402);background: -webkit-gradient(linear, left top, left bottom, from(#ffb84f), to(#ff8402));}
