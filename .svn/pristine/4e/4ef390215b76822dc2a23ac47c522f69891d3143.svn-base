.cart-logistics{position: absolute;right: 10px;top: 50%;margin-top:-12px ;color: #fff;font-size: 16px;}

.concern{border-radius: 5px;background: #2c7;position: relative;line-height: 32px;}
.concern:after{position: absolute;top: 8px;left: -8px;width: 0;height: 0;content: " ";border-top: 5px solid transparent;border-right: 10px solid #2c7;border-bottom: 5px solid transparent;}

.goods-style-list>a{float: left;margin:0 5px 5px 0;position: relative;height: 24px;line-height: 24px; padding: 2px 10px;border: 1px solid #999;color: #999;background: #fff; border-radius: 2px;}
.goods-style-list>a.empty{background: #f2f2f2;color: rgba(0,0,0,0.2);border-color:rgba(0,0,0,0.2) ;}
.goods-style-list>a.active{border-color: #509ee0;color: #509ee0;}
.goods-style-list>a.active:after{position: absolute;width: 8px;height: 8px;color: #fff; background: #509ee0;content: "\e645";	display: inline-block;border-radius:4px 0  0 0;	font-family: iconfont;bottom: 0;right: 0;	font-style: normal;	font-weight: bold;	font-size: 8px;	line-height: 10px;	text-decoration: none;	-webkit-font-smoothing: antialiased;}
