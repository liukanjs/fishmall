#wrap{position: fixed;left: 0;right: 0;top: 0;bottom: 0;background: #fff;z-index: 5}
#nprogress{pointer-events: none}
#nprogress .bar{background: #29d;position: fixed;z-index: 100;top: 0;left: 0;width: 100%;height: 4px}
#nprogress .peg{display: block;position: absolute;right: 0;width: 100px;height: 100%;box-shadow: 0 0 10px #29d, 0 0 5px #29d;opacity: 1.0;-webkit-transform: rotate(3deg) translate(0px, -4px);-ms-transform: rotate(3deg) translate(0px, -4px);transform: rotate(3deg) translate(0px, -4px)}
#nprogress .spinner{display: block;position: fixed;z-index: 100;top: 15px;right: 15px}
#nprogress .spinner-icon{position: relative;top: 2px;width: 12px;height: 12px;box-sizing: border-box;border: solid 2px transparent;border-top-color: #29d;border-left-color: #29d;border-radius: 50%;-webkit-animation: nprogress-spinner 400ms linear infinite;animation: nprogress-spinner 400ms linear infinite}
@-webkit-keyframes nprogress-spinner{0%{-webkit-transform: rotate(0deg)}100%{-webkit-transform: rotate(360deg)}}
@keyframes nprogress-spinner{0%{transform: rotate(0deg)}100%{transform: rotate(360deg)}}
#nprogress{position: fixed;top: 0;right: 0;bottom: 0;left: 0;background-color: #f7f7f7;z-index: 999;}
#nprogress .progress{border: 2px solid rgba(0, 0, 0, 0.5);-webkit-border-radius: 10px;-moz-border-radius: 10px;border-radius: 10px;}
#nprogress .splash{position: absolute;top: 40%;left: 0;right: 0;margin: auto;}
#nprogress .splash img{display: block;margin-left: auto;margin-right: auto;height: 100px;width: 100px;}
#nprogress .card{background-color: #f7f7f7;width: 200px;}
#nprogress .mybar{background: rgba(0, 0, 0, 0.5);height: 15px;border-radius: 5px;overflow: hidden;z-index: 999;margin: 3px;}
#nprogress .progress{height: 25px;
width: 200px;
overflow: hidden;
z-index: 1000;}
#nprogress .text-loading{font-size: 12px;}
#nprogress .spinner-div{margin-top: 5px;text-align: center;}
#nprogress .spinner-div div{display: inline-block;}